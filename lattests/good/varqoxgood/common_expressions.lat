class C {
	string s;

	boolean empty() {
		return s == "";
	}

	void print() {
		printString(s);
	}

	void extend(string s) {
		self.s = self.s + s;
	}
}

void assert(boolean b) {
	if (!b) error();
}

void test6() {
	C c = new C;
	assert(c.empty());
	c.print();
	c.extend("abc");
	assert(!c.empty());
	c.print();
	c.extend(";efg");
	assert(!c.empty());
	c.print();
}

