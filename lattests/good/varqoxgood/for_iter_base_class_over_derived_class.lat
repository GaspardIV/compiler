	X[] arrx = new X[2];
	arrx[0] = new X;
	arrx[1] = new Y;
	int iter = 0;
	for (X x : arrx) {
		x.foo();
		if (iter == 0) {
			if (x.x != 42) error();
		} else {
			if (x.x != 142) error();
		}
		iter++;
	}

	Y[] arry = new Y[1];
	arry[0] = new Y;
	for (X x : arry) {
		x.foo();
