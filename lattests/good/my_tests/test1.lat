void funkcja6() {
// nasted while
    printString("========");
    int i = 0, j = 0;
    while (i < 5) {
        while (j < 5) {
            printInt(j);
            printInt(i);
            j++;
        }
        printInt(i);
        printInt(j);
        i++;
    }
}
